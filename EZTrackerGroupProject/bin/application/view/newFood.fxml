<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.paint.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="foodScene" prefHeight="500.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controller.newFood">
   <children>
      <Button fx:id="backToUser" layoutX="14.0" layoutY="457.0" mnemonicParsing="false" onAction="#toUserScene" prefHeight="9.0" prefWidth="70.0" stylesheets="@../view/application.css" text="Back " textFill="#4f3a3a">
      </Button>
      <TextField fx:id="calories" layoutX="91.0" layoutY="103.0" prefHeight="60.0" prefWidth="165.0" promptText="Enter number of calories" />
      <TextField fx:id="carbs" layoutX="91.0" layoutY="189.0" prefHeight="60.0" prefWidth="165.0" promptText="Enter carbs in grams" />
      <TextField fx:id="fat" layoutX="91.0" layoutY="279.0" prefHeight="60.0" prefWidth="165.0" promptText="Enter fat in grams" />
      <TextField fx:id="protein" layoutX="91.0" layoutY="359.0" prefHeight="60.0" prefWidth="165.0" promptText="Enter protein in grams" />
      <Label layoutX="22.0" layoutY="103.0" prefHeight="17.0" prefWidth="103.0" text="Calories" />
      <Label layoutX="21.0" layoutY="189.0" prefHeight="17.0" prefWidth="41.0" text="Carbs" />
      <Label layoutX="22.0" layoutY="279.0" text="Fat" />
      <Label layoutX="17.0" layoutY="359.0" prefHeight="17.0" prefWidth="48.0" text="Protein" />
      <Button fx:id="addMeal" layoutX="325.0" layoutY="337.0" mnemonicParsing="false" onAction="#addToMeal" prefHeight="36.0" prefWidth="103.0" text="Add To Meal" />
      <Button fx:id="mealComplete" layoutX="325.0" layoutY="379.0" mnemonicParsing="false" onAction="#completeMeal" prefHeight="36.0" prefWidth="103.0" text="Done With Meal" />
      <Label fx:id="currentUser" layoutX="377.0" layoutY="461.0" prefHeight="17.0" prefWidth="73.0" text="//" />
      <TextField fx:id="foodName" layoutX="91.0" layoutY="25.0" prefHeight="60.0" prefWidth="165.0" promptText="Enter name of food" />
      <Label layoutX="22.0" layoutY="25.0" prefHeight="17.0" prefWidth="103.0" text="Food" />
      <TextArea fx:id="output" layoutX="279.0" layoutY="25.0" prefHeight="290.0" prefWidth="195.0" promptText="Will Output a Confirmation Msg" />
   </children>
</AnchorPane>
